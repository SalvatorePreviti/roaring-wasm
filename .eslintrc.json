{
  "extends": ["@balsamic"],
  "plugins": ["node", "import"],
  "rules": {
    "global-require": 0,
    "@typescript-eslint/no-unsafe-declaration-merging": 0
  },
  "overrides": [
    {
      "files": ["index.js"],
      "rules": {
        "node/no-unpublished-require": 0,
        "import/no-unresolved": 0
      }
    }
  ]
}
