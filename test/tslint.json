{
  "extends": "../tslint.json",
  "rules": {
    "no-empty": false,
    "missing-jsdoc": false,
    "no-implicit-dependencies": false,
    "no-console": false,
    "typedef": false,
    "completed-docs": false,
    "promise-function-async": false,
    "import-name": [
      true,
      {
        "idisposable": "IDisposable"
      }
    ]
  }
}
